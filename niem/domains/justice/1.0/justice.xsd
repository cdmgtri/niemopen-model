    <xsd:annotation>
      <xsd:documentation>BAC Test Result (2 digits).</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/1.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:pattern value="\d{2}">
        <xsd:annotation>
          <xsd:documentation>BAC Test Result (2 digits).</xsd:documentation>
        </xsd:annotation>
      </xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>
