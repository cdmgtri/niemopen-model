    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DeconCapacityCommentText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>One or more comments</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="DepartureDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The date and time that the resource departs the route location.  Used for resource en route tracking</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EMSCapacity" type="em:TriagePatientCountType" nillable="true">
