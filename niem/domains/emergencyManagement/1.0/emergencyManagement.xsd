      <xsd:documentation>Whether the hospital has activated its Emergency Operations Plan (EOP) </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EOCPlanCommentText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>One or more comments</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EOCStatusCode" type="have:EOCStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Whether the Emergency Operations Center (EOC) is currently operating.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EOCStatusCommentText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>One or more comments</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="EmergencyDepartmentStatus" type="em:EmergencyDepartmentStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The container of all of the elements related to the emergency department status.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ExplicitAddress" type="em:ExplicitAddressType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An identifier of an explicit recipient.  Identifies human parties, systems, services, or devices that are all potential recipients of the distribution message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ExplicitAddressSchemeText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifies the distribution addressing scheme used.   MUST be a properly formed -escaped if necessary- XML string. Examples for this type of distribution includes - email, military USMTF, etc. . .  
</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ExplicitAddressValueText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A properly formed -escaped if necessary- XML string denoting the addressees value</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FacialServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of facial surgical services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FacilityOperationsCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The status of supplies necessary for facility operations</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FacilityStatusCode" type="have:FacilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Indicator of the status of the facility status.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FacilityStatusCommentText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>One or more comments</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="FundingInformationText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Provides additional information on the funds that will pay for the resource (A textual description of funding sources or distribution)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="HandServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of hand surgery services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="HomeDispatch" type="c:OrganizationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The home dispatch the resource is assigned to</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="HomeUnit" type="c:OrganizationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The home unit the resource is assigned to</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Hospital" type="em:HospitalType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The container element for reporting status of a hospital. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="HospitalBedCapacityStatus" type="em:HospitalBedCapacityStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The container of all of the elements related to the hospital bed capacity and status. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="HospitalFacilityStatus" type="em:HospitalFacilityStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The container of all of the elements related to the status of the facility  </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="HospitalResourcesStatus" type="em:HospitalResourcesStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The container of all of the elements related to the operations of the facility  </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="HospitalStatus" type="em:HospitalStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The top level container element for reporting status of any number of hospitals. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="InfectiousDiseasesServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of infectious diseases services. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="InventoryRefreshDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Date and time that resource inventory counts were last updated</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LaborDeliveryServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Sub-type element of the OBGYN services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Location" type="em:LocationType" nillable="true"/>
  <xsd:element name="LocationAltitudeMeasure" type="u:LengthMeasureType" nillable="true"/>
  <xsd:element name="LocationAugmentation" type="em:LocationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>An augmentation that applies to type c:LocationType</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/common/1.0" i:name="LocationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LocationDepthMeasure" type="u:LengthMeasureType" nillable="true"/>
  <xsd:element name="LocationDirectionsText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Directions to the location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="LocationInformationText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Necessary information about a location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageContentCategoryCode" type="edxl:ContentCategoryCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Specifies the purpose / type of resource content / payload being sent </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageRequestID" type="u:IDType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Each EDXL resource message contains an identifier &quot;Request Number&quot; that uniquely identifies each resource message. The EDXL Distribution Element contains the &quot;Distribution ID&quot;, which identifies the &quot;container&quot; for the distribution message information.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MessageSentDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The system stamped date and time the resource message was sent.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MorgueCapacityCode" type="have:MorgueCapacityCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Indicator of the status of the morgue capacity.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="MorgueCapacityCommentText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>One or more comments</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NavigationInstructionsText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Instructions that define how to get to the report to location</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NeonatologyServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of neonatology services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NeuroSurgeryServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of Neurosurgery services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="NeurologyServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of neurology services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OBGYN" type="em:OBGYNType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of OBGYN services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OBGYNGeneralServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true"/>
  <xsd:element name="OBGYNServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The Sub-type element of the OBGYN services. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrganizationGeoLocation" type="em:OrganizationGeoLocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The container element for specifying the geo-coded address. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrganizationIDProviderName" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of the provider that has provided the identification scheme. This could also be the name a particular identification list. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrganizationLocationAugmentation" type="em:OrganizationLocationAugmentationType" substitutionGroup="s:Augmentation">
    <xsd:annotation>
      <xsd:documentation>The container element for the specifying the location of the organization.</xsd:documentation>
      <xsd:appinfo>
        <i:AppliesTo i:namespace="http://niem.gov/niem/common/1.0" i:name="LocationType"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OriginatingMessageID" type="u:IDType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Each EDXL resource message contains an identifier that uniquely identifies each resource message. This element identifies the ResourceMessageID of the original message to which this message refers. This Message ID is an EDXL-RM Message ID, not an EDXL-DE </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrthopedicServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of orthopedic services. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OrthopedicsServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of Orthopedic surgical services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Owner" type="c:OrganizationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>An organization with a specific range of authority for specified resource(s)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="OwnerInformation" type="em:OwnerInformationType" nillable="true"/>
  <xsd:element name="PediatricServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Sub-type element of the psychiatric services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PediatricsGeneralServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of Pediatrics general surgical services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Permit" type="em:PermitType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A permit.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PermitCategoryText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A type of permit.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PermitID" type="u:IDType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A permit number.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Psychiatric" type="em:PsychiatricType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of psychiatric services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="PsychiatricGeneralServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true"/>
  <xsd:element name="ReimplantationServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of reimplantation surgical services.  </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ReportingInstructionsText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of the party that the requested item is to report to when they arrive at the incident.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RequestAcceptDeclineCode" type="edxl:ResponseCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Used to accept or decline a Request, Response, Unsolicited Offer, or a Request Return.  &quot;Decline&quot; indicates the request, response or offer is not accepted.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RequestResponse" type="em:RequestResponseType" nillable="true"/>
  <xsd:element name="Resource" type="em:ResourceType" nillable="true"/>
  <xsd:element name="ResourceActualArrivalDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Actual date and time of arrival of the resource a Report To Location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceActualDepartureDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Actual date and time when the resource departs from its current location for transit to a Report To Location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceAnticipatedIncidentAssignmentText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Anticipated function, task, job, or role to be provided by the requested resource.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceAnticipatedReturnDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When the resource is expected to be returned or DEMOBed.  Completed for a Request Resource.  This element with the Requested Arrival Date / Time provides the estimated duration of resource deployment.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceAvailableDate" type="u:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When the resource is available for use.  Completed in response to a resource message.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceCommittedDate" type="u:DateType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When specified resource is committed to a request or order.  Completed in response to a resource message.  Specified resource is no longer available to be applied to other resource requests</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceCurrentLocation" type="em:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The location where the specified resource is physically located at the time of the request.  Uses the Location message elements to specify the Current Location.  **May also be used to track current location of an in-transit resource</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceDepartureLocation" type="em:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The location where the specified resource is departing from when transported to or from an Incident.  Uses the Location message elements to specify the Place of Departure</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceDescriptionText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Free Text alternative to &quot;Resource Keyword&quot; (or may be used in addition).  Any description of resource or resource characteristics, situation requiring resource assistance, statement of mission resource must satisfy, or RFI text.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceDispositionKeywordValue" type="em:ValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Any value from a discrete managed list, used to specify the general state of a resource if known., in the form:
&lt;ResourceStatuskeyword&gt;
  &lt;valueListUrn&gt;valueListUrn&lt;/valueListURN&gt;
  &lt;value&gt;value&lt;/value&gt;
&lt;/ResourceStatuskeyword&gt;
where the content of valueListUrn is the Uniform Resource Name of a published list of values and definitions, and the content of value is a string denoting the value itself .  Multiple instances MAY occur.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceEstimatedArrivalDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When the resource is expected to arrive at its Report To Location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceEstimatedDepartureDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When the resource is expected to depart from its current location for transit to a Report-to Location</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceFundCodeText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Identifies the funds that will pay for the resource - Identified in support of NIMS Resource Management Guide NIC-GDL0004</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceID" type="u:IDType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>This identifier (if available) must follow the resource / resource description during the entire trail of messages about that resource.  May be selected via the &lt;Resource Keyword&gt; if available.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceInformation" type="em:ResourceInformationType" nillable="true"/>
  <xsd:element name="ResourceJurisdictionText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A geopolitical area in which an organization, person, or object has a specific range of authority for specified resources</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceKeywordValue" type="em:ValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Any value from a discrete managed list, used to specify a resource, in the form:
&lt;resourcekeyword&gt;
  &lt;valueListUrn&gt;valueListUrn&lt;/valueListURN&gt;
  &lt;value&gt;value&lt;/value&gt;
&lt;/resourcekeyword&gt;
where the content of valueListUrn is the Uniform Resource Name of a published list of values and definitions, and the content of value is a string denoting the value itself.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceKind" type="em:ResourceKindType" nillable="true"/>
  <xsd:element name="ResourceLocationDetails" type="em:ResourceLocationDetailsType" nillable="true"/>
  <xsd:element name="ResourceMessageStatusKeywordValue" type="em:ValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description or Code used to identify the message status of a resource message.  
Any value from a discrete managed list, used to specify a Request Status.  The content of valueListUrn is the Uniform Resource Name of a published list of values and definitions, and the content of value (StatusText) is a string denoting the value itself .  Multiple instances MAY occur within a single ResourceDataFrame block.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceModeOfTransportationText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Method or mode used to transport the resource to or from the incident.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceName" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A name or title of the resource.  May be selected via the &lt;Resource Keyword&gt; if available.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceOrderID" type="u:IDType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number or ID assigned by the ordering system or personnel meeting the request for resources that has been made.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourcePriceQuoteAmount" type="u:AmountType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Quoted cost to acquire a desired resource.  Complete in response to a Request Resource Quote</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceQuantity" type="niem-xsd:nonNegativeInteger" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number of resources</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceReportToLocation" type="em:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Where the requested resource is to report-to, or checks in at the destination.  This location may be different than the Requested Location.  Completed for Resource requests, returns, etc.   Uses the Location message elements to specify the Report To Location.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceRequestedArrivalDateTime" type="niem-xsd:dateTime" nillable="true">
    <xsd:annotation>
      <xsd:documentation>When the resource is needed.  Completed for Resource requests, returns, etc.  </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceRequestedLocation" type="em:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Where the requested resource is needed.  Completed for Resource requests, returns, etc.   Uses the Location message elements to specify the Location</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceRestrictionsText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Description of a condition governing the availability of resources, e.g. condition for number of beds available may be &quot;if patents have insurance&quot;.  This may be thought of as a term/condition or a restriction on availability.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceSchedule" type="em:ResourceScheduleType" nillable="true"/>
  <xsd:element name="ResourceSpecialRequirementsText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A description of any special needs related to the requested resource (e.g. must carry protective </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceStatusKeywordValue" type="em:ValueType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Any value from a discrete managed list, used to specify the general state of a resource if known.  where the content of valueListUrn is the Uniform Resource Name of a published list of values and definitions, and the content of value is a string denoting the value itself .  Multiple instances MAY occur.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResourceTracking" type="em:ResourceTrackingType" nillable="true"/>
  <xsd:element name="ResponseCode" type="edxl:ResponseCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Used to accept or decline a Request, Response, Unsolicited Offer, or a Request Return.  &quot;Decline&quot; indicates the request, response or offer is not accepted.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ResponseReasonCode" type="edxl:ResponseCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Explanation for a declined Request, Response, Unsolicited Offer, or a Request Return.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="RouteLocation" type="em:LocationType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A location along the delivery route for a resource which may be origin, interim, or destination location.  Used for resource en route tracking</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SecurityStatusCode" type="have:SecurityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Indicator of the status of security procedures in the hospital.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SecurityStatusCommentText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>One or more comments</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ServiceCoverageStatus" type="em:ServiceCoverageStatusType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Container element of all the elements of service coverage. This includes both the necessary staff and facilities. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SpinalServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of spinal surgical services.  </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="StaffingCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Indicator of the status of staffing at the hospital.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SubCategoryBedCapacity" type="em:SubCategoryBedCapacityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The container element for the subcategories of each bed type.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SubCategoryBedCategoryText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of the sub-category bed type</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Surgery" type="em:SurgeryType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of general surgery services. </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="SurgeryGeneralServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true"/>
  <xsd:element name="TriageBlackQuantity" type="u:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number of deceased victims</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TriageGreenQuantity" type="u:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number of victims with minor needs</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TriageRedQuantity" type="u:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number of victims with immediate needs.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="TriageYellowQuantity" type="u:QuantityType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>Number of victims with delayed needs</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Value" type="em:ValueType" nillable="true"/>
  <xsd:element name="ValueListURN" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The name of the certified list maintained by the Community of Interest (COI) for the value referenced.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="ValueText" type="u:TextType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>A Value from a certified list maintained by the Community of Interest (COI) for the referenced element.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VascularServiceCoverageStatusCode" type="have:AvailabilityStatusCodeType" nillable="true">
    <xsd:annotation>
      <xsd:documentation>The availability of vascular surgical services.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
</xsd:schema>
