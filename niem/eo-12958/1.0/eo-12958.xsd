    <xsd:simpleContent>
      <xsd:extension base="eo-12958:SecurityClassificationCodeSimpleType">
        <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
