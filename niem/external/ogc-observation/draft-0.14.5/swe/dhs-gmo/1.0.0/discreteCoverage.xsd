		</annotation>
		<sequence>
			<!--
<element name="element" type="swe:CV_AbstractGeometryValuePairPropertyType"
maxOccurs="unbounded"/>
-->
		</sequence>
	</complexType>
	<!-- ........... -->
	<element name="CV_AbstractDiscreteCoverage" type="swe:CV_AbstractDiscreteCoverageType" abstract="true">
<annotation>
